    <value-set
        name="virtual">
        <formal-name>Virtual</formal-name>
        <description>Indicates if the asset is virtual.</description>
        <binding
            pattern="inventory-item/prop[@name='virtual']"/>
        <binding
            pattern="component/prop[@name='virtual']"/>
        <allowed-values
            allow-other="no">
            <enum
                short-label="Y"
                value="yes">Yes</enum>
            <enum
                short-label="N"
                value="no">No</enum>
        </allowed-values>
