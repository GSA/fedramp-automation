    <value-set
        name="scan-type">
        <formal-name>Scan Type</formal-name>
        <description>Identifies the type of scan.</description>
        <binding
            pattern="component/prop[@name='scan-type'][@ns='http://fedramp.gov/ns/oscal']"/>
        <binding
            pattern="inventory-item/prop[@name='scan-type'][@ns='http://fedramp.gov/ns/oscal']"/>
